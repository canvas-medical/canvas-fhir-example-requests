meta {
  name: Search CarePlan
  type: http
  seq: 2
}

get {
  url: {{base_url}}/CarePlan?patient=Patient/{{patient_id}}
  body: none
  auth: inherit
}

params:query {
  patient: Patient/{{patient_id}}
  ~category: http://hl7.org/fhir/us/core/CodeSystem/careplan-category|assess-plan
}

auth:bearer {
  token: {{fhir_access_token}}
}
